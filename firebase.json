{
    "hosting": [
        {
            "target": "qa",
            "public": "dist/meddi-supply/browser",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "headers": [
                {
                    "source": "**/*.@(js|css)",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "max-age=31536000"
                        }
                    ]
                },
                {
                    "source": "**/*.@(jpg|jpeg|gif|png|svg|webp)",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "max-age=31536000"
                        }
                    ]
                }
            ]
        },
        {
            "target": "prod",
            "public": "dist/meddi-supply/browser",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "headers": [
                {
                    "source": "**/*.@(js|css)",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "max-age=31536000"
                        }
                    ]
                },
                {
                    "source": "**/*.@(jpg|jpeg|gif|png|svg|webp)",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "max-age=31536000"
                        }
                    ]
                }
            ]
        }
    ]
}